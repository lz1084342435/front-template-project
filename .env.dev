# Delete console and debugger
VITE_DROP_CONSOLE_DEBUGGER = false

# Cross-domain proxy, you can configure multiple
# Please note that no line breaks
VITE_PROXY = [["/api", "http://localhost:4444"]]

VITE_GLOBAL_PREFIX = '/api'

VITE_LEGACY = false

VITE_ANALYSIS = false

VITE_COMPRESS = false

VITE_USE_MOCK = true

# 开发 & 测试
VITE_BUILD_MODE = "development"
